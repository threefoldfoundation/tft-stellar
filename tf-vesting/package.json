{
  "name": "tf-vesting",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "npx eslint ./src && vite build",
    "dev": "vite dev --port=8080",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "lint": "yarn format && npx eslint --fix --ext .js,.ts,.svelte ./src",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json|svelte)\""
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^2.0.3",
    "@tsconfig/svelte": "^3.0.0",
    "@types/uuid": "^9.0.1",
    "eslint": "^8.35.0",
    "prettier-plugin-svelte": "^2.9.0",
    "svelte": "^3.55.1",
    "svelte-check": "^2.10.3",
    "tslib": "^2.5.0",
    "typescript": "^4.9.3",
    "vite": "^4.2.0"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "axios": "^0.27.2",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-svelte3": "^4.0.0",
    "prettier": "^2.8.4",
    "stellar-sdk": "10.1.2",
    "svelte-github-login": "^1.3.1",
    "svelte-navigator": "^3.2.2",
    "sveltestrap": "^5.10.0",
    "uuid": "^9.0.0",
    "vite-plugin-node-polyfills": "^0.7.0"
  }
}
